<div id="journal" class="text-left paddsection">

    <div class="container">
        <div class="section-title text-center">
            <a href="/blog">
                <h2>journal</h2>
            </a>

        </div>
    </div>

    <div class="container">
        <div class="journal-block">
            <div class="row">
                <%for(let blog of blogs){ %>
                    <div class="col-lg-4 col-md-6">
                        <div class="journal-info">

                            <a href="/blog/<%=blog._id%>"><img src=<%=blog.thumbnail.mini%> class="img-responsive"
                                alt="img"></a>

                            <div class="journal-txt">

                                <h4><a href="/blog/<%=blog._id%>">
                                        <%=blog.title%>
                                    </a></h4>
                                <p class="separator">
                                    <%=blog.description%>
                                </p>

                            </div>

                        </div>
                    </div>
                    <% }%>
            </div>
        </div>
    </div>

</div>